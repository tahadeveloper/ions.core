{
  "name": "ionzile/core",
  "description": "Ions core library",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Ions\\": "src/"
    },
    "classmap": [
      "src/commands"
    ]
  },
  "authors": [
    {
      "name": "ahmedhamdi",
      "email": "taha.developer@outlook.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-openssl": "*",
    "ext-zip": "*",
    "cartalyst/sentinel": "^v6.0.1",
    "illuminate/cache": "v9.52.4",
    "illuminate/console": "v9.52.4",
    "illuminate/database": "v9.52.4",
    "illuminate/events": "v9.52.4",
    "illuminate/filesystem": "v9.52.4",
    "illuminate/http": "v9.52.4",
    "illuminate/pagination": "v9.52.4",
    "illuminate/validation": "v9.52.4",
    "lcobucci/jwt": "^4.3.0",
    "monolog/monolog": "^2.10.0",
    "peppeocchi/php-cron-scheduler": "^4.0",
    "symfony/config": "^6.4.14",
    "symfony/mailer": "^6.4.18",
    "symfony/routing": "^6.4.18",
    "symfony/security-csrf": "^6.4.13",
    "symfony/translation": "^6.4.19",
    "symfony/yaml": "^6.4.20",
    "vlucas/phpdotenv": "^5.6.1",
    "league/flysystem": "^3.29.1",
    "league/flysystem-aws-s3-v3": "^3.29"
  },
  "require-dev": {
    "pestphp/pest": "^v1.23.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "platform": {
      "php": "8.2"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
