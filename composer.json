{
    "name": "ionzile/core",
    "description": "Ionzile core library",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ions\\": "src/"
        },
        "classmap": [
            "src/commands"
        ]
    },
    "authors": [
        {
            "name": "ahmedhamdi",
            "email": "taha.developer@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "cartalyst/sentinel": "^v6.0",
        "illuminate/cache": "v9.0.2",
        "illuminate/console": "v9.0.2",
        "illuminate/database": "v9.0.2",
        "illuminate/events": "v9.0.2",
        "illuminate/filesystem": "v9.0.2",
        "illuminate/http": "v9.0.2",
        "illuminate/pagination": "v9.0.2",
        "illuminate/validation": "v9.0.2",
        "lcobucci/jwt": "^4.1.5",
        "monolog/monolog": "^2.3.5",
        "peppeocchi/php-cron-scheduler": "^4.0",
        "symfony/config": "^v6.0.6",
        "symfony/mailer": "^6.0.6",
        "symfony/routing": "^v6.0.6",
        "symfony/security-csrf": "^v6.0.6",
        "symfony/translation": "^v6.0.6",
        "symfony/yaml": "^6.0.6",
        "vlucas/phpdotenv": "^v5.4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
