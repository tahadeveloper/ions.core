{
  "name": "ionzile/core",
  "description": "Ionzile core library",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Ions\\": "src/"
    },
    "classmap": [
      "src/commands"
    ]
  },
  "authors": [
    {
      "name": "ahmedhamdi",
      "email": "taha.developer@outlook.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-openssl": "*",
    "ext-zip": "*",
    "cartalyst/sentinel": "^v6.0",
    "illuminate/cache": "v9.5.1",
    "illuminate/console": "v9.5.1",
    "illuminate/database": "v9.5.1",
    "illuminate/events": "v9.5.1",
    "illuminate/filesystem": "v9.5.1",
    "illuminate/http": "9.5.1",
    "illuminate/pagination": "v9.5.1",
    "illuminate/validation": "v9.5.1",
    "lcobucci/jwt": "^4.1.5",
    "monolog/monolog": "^2.4.0",
    "peppeocchi/php-cron-scheduler": "^4.0",
    "symfony/config": "^v6.0.3",
    "symfony/mailer": "^6.0.5",
    "symfony/routing": "^v6.0.5",
    "symfony/security-csrf": "^v6.0.3",
    "symfony/translation": "^v6.0.6",
    "symfony/yaml": "^6.0.3",
    "vlucas/phpdotenv": "^v5.4.1"
  },
  "require-dev": {
    "pestphp/pest": "^1.21.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
